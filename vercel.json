{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/folders/(.*)",
        "dest": "api/folders.js"
      },
      {
        "src": "/api/links/(.*)",
        "dest": "api/links.js"
      },
      {
        "src": "/api/users/(.*)",
        "dest": "api/users.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "api/index.js"
      }
    ]
  }